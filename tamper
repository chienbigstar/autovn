function AutoRedeem() {
  return false;
  if (goRedeemPage && paypalEmail !== "") {
    if (window.location.href.includes("prizes")) {
      var c = parseFloat(document.getElementsByClassName("gift-value")[0].innerHTML.replace("$", "").split(" ")[0]);
      if (c > 2) {
        document.getElementsByClassName("themeBtn small redeemButton")[0].click();
        setTimeout(function() {
          var a = document.getElementById("userEmail");
          var b = document.getElementById("subGiftCard");
          a.value = paypalEmail;
          if (a.value !== "") {
            b.click()
          }
          setTimeout(function() {
            Cookies.set(redeemTodayCookie, "0");
            window.location.href = commonAcrossPages.siteUrl
          }, 3000)
        }, 1000)
      } else {
        setTimeout(function() {
          Cookies.set(redeemTodayCookie, "0");
          window.location.href = commonAcrossPages.siteUrl
        }, 1000)
      }
    } else {
      window.location.href = commonAcrossPages.siteUrl + "/prizes"
    }
  } else {
    return false
  }
  return goRedeemPage
}

function getVideoCode(a) {
  return a.split("?")[0].split("/")[4]
}

function GuesseResult(e, f, g) {
  setTimeout(function() {
    var d = getVideoCode(document.getElementById("video_player").src);
    console.log("Video Key : " + d);
    firebase.database().ref('/answers/' + d).once('value').then(function(a) {
      var b = a.val();
      if (b !== null) {
        console.log("Key Found : " + d + " - " + b);
        var c = false;
        for (var i = 0; i < e.length; i++) {
          if (e[i].innerHTML.includes(b)) {
            e[i].click();
            c = true;
            break
          }
        }
        if (!c) {
          ChooseRandom(d, e, f, g)
        } else {
          window.console.log('success');
          setTimeout(function() {
            if (f.innerHTML.toLowerCase().match(/correct|Congrats|You earned|success|Congrats/i)) {
              downCoin(1);

              if (!shutdownAppByReachVideo()) {
                window.location.href = g;
              }
            } else if (f.innerHTML.includes("wrong")) {
              ChooseRandom(d, e, f, g);
            }
          }, 3000)
        }
      } else {
        ChooseRandom(d, e, f, g)
      }
    })
  }, 3000)
}

function downCoin(value) {
  remainCoins = remainCoins - value;
  var data = {}; data[autovnKey.trim()] = remainCoins;
  firebase.database().ref('/autovnKey').update(data);
}

function shutdownAppByReachVideo() {
  if (useAutoShutdownVideo) {
    countVideoCorrect++;
    sessionStorage[countVideoCorrectSession] = countVideoCorrect;

    if (countVideoCorrect >= autoShutdownVideo) {
      sessionStorage[countVideoCorrectSession] = 0;
      window.location.href = 'https://www.google.com.vn/';
      return true;
    }
  }
  return false;
}

