  var a = document.createElement('div');
  a.innerHTML = '<br>'+
  'AUTOVN Key: <input type="text" id="autovnKey" />'+
  '<p>'+
   '<p>' + 'Shutdown at:' + '<br>' +
   '<input style="transform: scale(1.5);" type="checkbox" id="useAutoShutdownVideo" class="autoShutdown"> <input id="autoShutdownVideo" type="number" min="1" style="width:40px"/> (videos)' +
    '<br><br>' +
    '<br><button id="btnSetApi" type="button">Update</button>' +
    '<p> Click update to save changes </p>' + '<p>  </p>';
  a.setAttribute('id', 'controlPanel');

  var d = document.createElement('div');
  d.innerHTML = c ? '<p> ==================================== </p>' +
   '<p>Page: <select id="select-page">'+
    '<option value="flamzy">flamzy</option>'+
    '<option value="baymack">baymack</option>'+
    '<option value="snuckls">snuckls</option>'+
    '<option value="outbuck">outbuck</option>'+
    '<option value="skylom">skylom</option>'+
    '<option value="vipbirdie">vipbirdie</option>'+
   '</select></p>'+
    '<p>Username : <input type="text" id="username" /></p>' +
   '<p>Days : <input type="text" id="rentDay" /></p>' +
     '<p>Pass : <input type="password" id="admin-password" /></p>' +
    '<br><button id="btnSetRent" type="button">***** Set Rent *****</button>' : '';
  d.setAttribute('id', 'adminTool');

  var b = $('body');
  b.prepend($('<div/>').attr('style', "color:black;font-size:12px;background-image:url('https://drive.google.com/uc?export=download&id=1bXJ5w5mxeKEoc2X8mustSwAXUFMbMZjO');border:2px black solid;border-radius:10px;font-size:box-shadow:2px 2px 2px 2px black;position:fixed;  top:0px; left:0; z-index:9999999999999 !important; width:250px; background-color:#FFCC99; text-align: center; ")
    .append('<div style="z-index: 999999999999 !important;border-radius:5px;background-color:#FF6600;font-size:18px;font-weight:bold">AUTOVN<span id="minimal" style="border-radius: 5px;cursor:pointer;padding: 0px 5px;background-color:red;float:right;border:1px black solid">-</span></div>')
    .append($('<div/>').attr('id', 'autoPanel')
      .append(a)
      .append($('<p/>').text("=========================="))
      .append($('<p/>').text("Coins: " + remainCoins + " coins").attr('style', 'color:black;font-weight:bold;font-size:18px;border:1px red solid').attr('id', 'remain-coin'))));

  document.getElementById("btnSetApi").addEventListener("click", onSetCaptchaApi, false);
  var e = document.getElementById("btnSetRent");
  if (e !== null) {
    e.addEventListener("click", onSetRentClick, false)
  }
  $('.btnapi').change(function () {
    if ($('#useAntiCaptcha').prop('checked')) {
      $('#use2Captcha').prop('checked', false);
    }
    if ($('#use2Captcha').prop('checked')) {
      $('#useAntiCaptcha').prop('checked', false);
    }
  });

  SetupCookie();
  minimal();
}

function onSetCaptchaApi(a) {
  // init element variables
  var b = document.getElementById("captchaApi");
  var c = document.getElementById("use2Captcha");
  var a = document.getElementById("antiCaptchaApi");
  var e = document.getElementById("useAntiCaptcha");
  var l = document.getElementById("autovnKey");

  var useASDVideo = document.getElementById("useAutoShutdownVideo");
  var aSDVideo = document.getElementById("autoShutdownVideo");

  var useAutoBet_e = document.getElementById("useAutoBet");

  // init cookie for main variables
  captchaApi = b.value;
  antiCaptchaApi = a.value;
  use2Captcha = c.checked;
  useAntiCaptcha = e.checked;
  autovnKey = l.value;
  useAutoBet = useAutoBet_e.checked;
  useAutoShutdownVideo = useASDVideo.checked;
  autoShutdownVideo = aSDVideo.value;

  // init cookie for api
  Cookies.set(apiCookie, b.value, {
    expires: 7
  });
  Cookies.set(use2CaptchaCookie, c.checked ? "1" : "0", {
    expires: 7
  });
  Cookies.set(antiCaptchaApiCookie, a.value, {
    expires: 7
  });
  Cookies.set(useAntiCaptchaCookie, e.checked ? "1" : "0", {
    expires: 7
  });
  Cookies.set(autovnKeyCookie, l.value, {
    expires: 7
  });

  // init cookie for autoshutdown
  Cookies.set(useAutoShutdownVideoCookie, useASDVideo.checked ? "1" : "0", {
    expires: 7
  });
  Cookies.set(autoShutdownVideoCookie, aSDVideo.value, {
    expires: 7
  });
  Cookies.set(useAutoBetCookie, useAutoBet_e.checked ? "1" : "0", {
    expires: 7
  });

}

function onSetRentClick(a) {
  var pass = $('#admin-password').val();
  if (pass === '@tklmc2mt') {
    var b = parseInt(document.getElementById("rentDay").value);
    var c = document.getElementById("username").value;
    var d = new Date();
    var e = new Date();
    e.setDate(d.getDate() + b);
    var f = e.getHours();
    var g = e.getDate();
    var h = e.getMonth() + 1;
    var i = e.getFullYear();
    var j = h + "/" + g + "/" + i + "/" + f;
    var page = $('#select-page').val();
    firebase.database().ref('/users/' + page + '/' + c).set(j)
  }
}

function SetupCookie() {
  // element variables
  var a = document.getElementById("captchaApi");
  var b = document.getElementById("use2Captcha");
  var g = document.getElementById("antiCaptchaApi");
  var h = document.getElementById("useAntiCaptcha");
  var l = document.getElementById("autovnKey");

  var useAutoShutdownVideo_e = document.getElementById("useAutoShutdownVideo");
  var autoShutdownVideo_e = document.getElementById("autoShutdownVideo");
  var useAutoBet_e = document.getElementById("useAutoBet");

  // setup cookie for 2captcha
  if ((typeof Cookies.get(apiCookie) === "undefined")) {
    Cookies.set(apiCookie, "");
    a.value = ""
  } else {
    a.value = Cookies.get(apiCookie)
  }
  if ((typeof Cookies.get(use2CaptchaCookie) === "undefined")) {
    Cookies.set(use2CaptchaCookie, "0");
    b.checked = false
  } else {
    var d = Cookies.get(use2CaptchaCookie) === "0" ? false : true;
    b.checked = d
  }

  // setup cookie for anticaptcha
  if ((typeof Cookies.get(antiCaptchaApiCookie) === "undefined")) {
    Cookies.set(antiCaptchaApiCookie, "");
    g.value = ""
  } else {
    g.value = Cookies.get(antiCaptchaApiCookie)
  }
  if ((typeof Cookies.get(useAntiCaptchaCookie) === "undefined")) {
    Cookies.set(useAntiCaptchaCookie, "0");
    h.checked = false
  } else {
    var d = Cookies.get(useAntiCaptchaCookie) === "0" ? false : true;
    h.checked = d
  }

  // setup cookie for autovnkey
  if ((typeof Cookies.get(autovnKeyCookie) === "undefined")) {
    Cookies.set(autovnKeyCookie, "");
    l.value = ""
  } else {
    l.value = Cookies.get(autovnKeyCookie)
  }

  // setup cookie for autoShutdown
  if ((typeof Cookies.get(autoShutdownVideoCookie) === "undefined")) {
    Cookies.set(autoShutdownVideoCookie, "");
    autoShutdownVideo_e.value = ""
  } else {
    autoShutdownVideo_e.value = Cookies.get(autoShutdownVideoCookie)
  }
  if ((typeof Cookies.get(useAutoShutdownVideoCookie) === "undefined")) {
    Cookies.set(useAutoShutdownVideoCookie, "0");
    useAutoShutdownVideo_e.checked = false
  } else {
    var d = Cookies.get(useAutoShutdownVideoCookie) === "0" ? false : true;
    useAutoShutdownVideo_e.checked = d
  }

  // setup cookie for auto bet
  if ((typeof Cookies.get(useAutoBetCookie) === "undefined")) {
    Cookies.set(useAutoBetCookie, "0");
    useAutoBet_e.checked = false
  } else {
    var d = Cookies.get(useAutoBetCookie) === "0" ? false : true;
    useAutoBet_e.checked = d
  }

  // setup value for main variables by monitor
  captchaApi = a.value;
  use2Captcha = b.checked;
  antiCaptchaApi = g.value;
  useAntiCaptcha = h.checked;
  autovnKey = l.value;
  useAutoShutdownVideo = useAutoShutdownVideo_e.checked;
  autoShutdownVideo = autoShutdownVideo_e.value;
  useAutoBet = useAutoBet_e.checked;
}
