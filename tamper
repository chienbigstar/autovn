function ChooseRandom(b, c, d, e) {
  var f = Math.floor(Math.random() * 4);
  c[f].click();
  var g = setInterval(function() {
    if (d.innerHTML.toLowerCase().match(/correct|Congrats|You earned|success|Congrats/i)) {
      var a = c[f].innerHTML;
      firebase.database().ref('/answers/' + b).set(a);
      console.log("Add New Key : " + b + " - " + a);

      remainCoins = remainCoins - 1;
      var data = {}; data[autovnKey.trim()] = remainCoins;
      firebase.database().ref('/autovnKey').update(data);

      if (!shutdownAppByReachVideo()) {
        clearInterval(g);
        window.location.href = e
      }

    } else if (d.innerHTML.includes("wrong")) {
      clearInterval(g);
      setTimeout(function() {
        window.location.href = e
      }, 1000)
    }
  }, 1000)
}

