function Initialize() {
  if (window.location.href.includes("https://www.youtube.com/embed/")) {
    var e = (window.location != window.parent.location) ? document.referrer : document.location.href;
    if (e.includes("outbuck") || e.includes("sharkday")  || e.includes("baymack") || e.includes("snuckls") || e.includes("skylom") || e.includes("flamzy") || e.includes("grandool") || e.includes("vipbirdie")) {
      setInterval(function() {
  	    player = document.querySelector('.html5-video-container');
  		if(player) player.style.display = 'none';
        var a = document.getElementsByClassName("ytp-large-play-button ytp-button")[0];
        var b = document.getElementsByTagName("video")[0];
        var c = b.currentTime > 0 && !b.paused && !b.ended && b.readyState > 2;
        if (b.paused) {
          a.click()
        }
      }, 3000)
    }
  } else if (window.location.href.includes("https://img.youtube.com/")) {
    InitializeFirebase();
    setTimeout(function() {
      var a = document.getElementsByTagName("img")[0];
      var b = getBase64Image(a);
      var c = a.src.split("/")[4];
      firebase.database().ref('/famzyAnswers/' + c).set(b);
      document.body.removeChild(a)
    }, 1000)
  } else if (window.location.href.includes('recaptcha')) {
    if (useAutoClickCaptcha) {
      var bt = window.document.querySelector('.recaptcha-checkbox-checkmark'); bt.click();
    }
  } else {
    SetupControlPanel(false);
    if (window.location.href.includes("videos")) {
      initDashboard();
      setTimeout(function() {
        var currentTime = window.document.querySelector('.video-page-current-duration');
        var currentTimeInt = parseInt(currentTime.innerText);
        if (!currentTime || currentTimeInt == 0) {
          var next = window.document.querySelector('#nextvideo');
          window.location.href = '/videos';
        }
      }, 20000);
    } else if (window.location.href.includes("entry")) {
      countVideoCorrect++;
      sessionStorage[countVideoCorrectSession] == countVideoCorrect;
      if (autovnKey && autovnKey !== '') {
        InitializeFirebase();
        firebase.database().ref('/autovnKey/' + autovnKey.trim()).once('value').then(function(a) {
          remainCoins = a.val();
          downCoin(1);
        })
      }
      window.location.href = '/videos';
    } else {
      window.location.href = '/videos';
    }
  }
}

function initDashboard() {
  InitializeFirebase();
  firebase.database().ref('checkUpdate').once('value').then(function(a) {
    setTimeout(function() {
      window.location.reload()
    }, 600000);

    window.alert = function() {};
    var d = a.val().needLogin;
    if (!d) {
      SetupAutoWithoutLogin()
    } else {
      LoginWithFacebookName()
    }
  })
}

function LoginWithFacebookName() {
  var a = document.getElementsByClassName("profile-name");
  if (!a[0] && !window.location.href.match(/videos/)) {
    window.location.href = getPageRef();
    return 1;
  }
  a = a[0].innerHTML.trim();
  FirebaseLogin(a)
}

function getPageName() {
  var href = window.location.href;
  if (href.match(/flamzy/i)) return 'flamzy';
  else if (href.match(/baymack/i)) return 'baymack';
  else if (href.match(/skylom/i)) return 'skylom';
  else if (href.match(/snuckls/i)) return 'snuckls';
  else if (href.match(/outbuck/i)) return 'outbuck';
  else if (href.match(/vipbirdie/i)) return 'vipbirdie';
  else if (href.match(/sharkday/i)) return 'sharkday';
  else return 'none';
}

function getPageRef() {
  var href = window.location.href;
  if (href.match(/flamzy/i)) return flamzyRef;
  else if (href.match(/baymack/i)) return baymackRef;
  else if (href.match(/skylom/i)) return skylomRef;
  else if (href.match(/snuckls/i)) return snucklsRef;
  else if (href.match(/outbuck/i)) return outbuckRef;
  else if (href.match(/vipbirdie/i)) return vipbirdieRef;
  else if (href.match(/sharkday/i)) return sharkdayRef;
  else return '/videos';
}

function FirebaseLogin(d) {
  if (!autovnKey || autovnKey === '') {
    var m = "Please input autovnKey or Contact : <a href='"+facebookLink+"'>" + facebookLink+'</a>';
    $('body').append('<div style="position: fixed; top: 0; right: 0; border: 1px red solid; color: red; background-color: white; z-index: 999999999999 !important; padding: 20px">'+m+'</div>');
    return 1;
  }
  firebase.database().ref('/autovnKey/' + autovnKey.trim()).once('value').then(function(a) {
    var c = a.val();
    if (c && c > 0) {
      remainCoins = c;
      $('#remain-coin').text('Coins: ' + remainCoins + ' coins');
      SetupAuto();
    } else {
      m = "Not enough coin !!! Contact : <a href='"+facebookLink+"'>" + facebookLink+'</a>';
      $('body').append('<div style="position: fixed; top: 0; right: 0; border: 1px red solid; color: red; background-color: white; z-index: 999999999999 !important; padding: 20px">'+m+'</div>');
    }
  })
}

function GetDiffDays(a, b) {
  var c = 24 * 60 * 60 * 1000;
  var d = Math.round(Math.abs((a.getTime() - b.getTime()) / (c)));
  return d
}

function SetupAutoWithoutLogin() {
  var a = document.getElementsByClassName("profile-name")[0].innerHTML.trim();
  var b = "No need to login";
  SetupAuto()
}

function SetupAuto() {
  setTimeout(AutoBaymack, 1000);
  setTimeout(AutoFlamzy, 1000);
  setTimeout(AutoSkylom, 1000);
  setTimeout(AutoSnuckls, 1000);
  setTimeout(AutoVipBirdie, 1000);
  setTimeout(AutoOutbuck, 1000);
  setTimeout(AutoSharkday, 1000);
}

function AutoSharkday() {
  if (window.location.href.includes("sharkday")) {
    if (!AutoRedeem()) {
      if (window.location.href.includes("videos")) {
        var b = document.getElementsByClassName("video-category video-category-questions-div")[0];
        var c = document.getElementsByClassName("captchaDivs")[0];
        var d = false;
        var e = setInterval(function() {
          if (c.style.display === "block" && !d) {
            d = true;
            SolveRecaptchaV2(captchaApi)
          } else if (b.style.display === "block") {
            var a = document.getElementsByClassName("border-btn");
            GuesseResult(a, document.getElementsByClassName("watch-vdo-msg")[0], "https://www.sharkday.com");
            clearInterval(e)
          }
        }, 2000)
      } else {
        if (useAutoBet) {
          window.location.href = "https://www.sharkday.com";
          var coin = window.document.querySelector('.button.class-for-instant-baymack-videos.get-coins');
          coin = parseInt(coin.innerHTML.match(/\d+/));
          if (coin >= 7) {
            var allBit = window.document.querySelectorAll('.btn.bid-now');
            var ran = Math.floor(Math.random()*(5-1+1)+1);
            allBit[ran - 1].click();
            sleep(3000);
          }
        }
        window.location.href = "https://www.sharkday.com/videos"
      }
    }
  }
}

