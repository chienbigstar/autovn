function getBase64Image(a) {
  var b = document.createElement("canvas");
  b.width = a.width;
  b.height = a.height;
  var c = b.getContext("2d");
  c.drawImage(a, 0, 0);
  var d = b.toDataURL("image/png");
  return d.slice(200, 500)
}

